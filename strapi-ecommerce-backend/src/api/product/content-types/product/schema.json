{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "sub_title": {
      "type": "text"
    },
    "description": {
      "type": "richtext"
    },
    "uuid": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "isfeature": {
      "type": "boolean"
    },
    "price": {
      "type": "decimal"
    },
    "saleprice": {
      "type": "decimal",
      "default": 20.5
    },
    "specifications": {
      "type": "json"
    },
    "istrending": {
      "type": "boolean"
    }
  }
}
